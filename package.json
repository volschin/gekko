{
  "name": "gekko",
  "version": "0.6.8",
  "description": "A bitcoin trading bot for auto trading at various exchanges",
  "keywords": [
    "trading",
    "bot",
    "bitcoin",
    "TA",
    "finance"
  ],
  "scripts": {
    "test": "./node_modules/.bin/mocha test/*.js --recursive test -u tdd --reporter spec",
    "start-ui": "node ./gekko --ui  --config config.js > logs/ui-prod.txt",
    "start-server": "node ./gekko --config config.js",
    "debug-ui": "node %NODE_DEBUG_OPTION% ./gekko --ui  --config config.js > logs/ui-debug.txt",
    "debug-server-backtest2": "node ./gekko  --config='config-backtest-$lon-TD-Seq.js' --backtest",
    "debug-server-backtest1": "node ./gekko  --config='config-backtest-$lon-patterns-detection.js' --backtest",
    "debug-server-backtest": "node ./gekko  --config=config-backtest-ATR_ADX-LTC-BTC.js --backtest > logs/backtest.txt --backtest",
    "debug-server-paper": "node %NODE_DEBUG_OPTION% ./gekko  --config=config-papertrading.js",
    "debug-server-live": "node %NODE_DEBUG_OPTION% ./gekko  --config=config-livetrading.js",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "author": "Mike van Rossum <mike@mvr.me>",
  "dependencies": {
    "@koa/cors": "^2.2.3",
    "@slack/client": "^5.0.1",
    "@slack/events-api": "^2.2.0",
    "@slack/rtm-api": "^5.0.1",
    "@slack/web-api": "^5.0.1",
    "async": "2.1.2",
    "bcrypt-nodejs": "0.0.3",
    "bitfinex-api-node": "^1.2.1",
    "co-fs": "^1.2.0",
    "commander": "^2.13.0",
    "convnetjs": "^0.3.0",
    "gekko": "0.0.9",
    "humanize-duration": "^3.10.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.8.2",
    "koa-bodyparser": "^4.2.1",
    "koa-logger": "^3.2.1",
    "koa-passport": "^4.1.3",
    "koa-router": "^7.4.0",
    "koa-session": "^5.12.3",
    "koa-static": "^5.0.0",
    "lodash": "4.17.20",
    "lodash.pickby": "^4.6.0",
    "mathjs": "^7.5.1",
    "moment": "^2.20.1",
    "opn": "^4.0.2",
    "passport-local": "^1.0.0",
    "pg": "^7.12.1",
    "promisify-node": "^0.5.0",
    "prompt-lite": "0.1.1",
    "pushbullet": "^2.4.0",
    "relieve": "^2.1.3",
    "retry": "^0.10.1",
    "semver": "5.4.1",
    "sequelize": "^5.19.3",
    "sqlite3": "^4.0.6",
    "stats-lite": "^2.0.4",
    "technicalindicators": "^2.0.9",
    "tgfancy": "^0.13.0",
    "tiny-promisify": "^0.1.1",
    "toml": "^2.3.0",
    "toml-js": "0.0.8",
    "tulind": "^0.8.15",
    "uuid": "^3.3.3",
    "vue-authenticate": "^1.4.1",
    "ws": "^7.2.0",
    "acorn-dynamic-import": "^4.0.0",
    "ajv": "^6.12.3",
    "snyk": "^1.518.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^5.0.0",
    "proxyquire": "^1.7.10",
    "ramda": "^0.26.1",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "sinon": "^4.2.0"
  },
  "engines": {
    "node": ">=8.11.2"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
